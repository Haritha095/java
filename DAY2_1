public class Day2
{
int marks;
String name;
int rollno;
Day2(String name,int rollno,int marks,String sub )
{
    this.name=name;
    this.rollno=rollno;
   this.marks=marks;
}
public void result()
{
    if(marks>50){
        System.out.println("Pass");

    }
    else{
        System.out.println("Fail");

    }
}}

